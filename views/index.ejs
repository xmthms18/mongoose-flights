<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

<h1>All Flights</h1>
<ul>
  <% flights.forEach((flights) => { %>
    <li>
      <%= flight.airline %> <%= flight.flightNo %>
      <a href="/flights/<%= flight._id %>">Details</a>
    </li>
  <% }); %>
</ul>

  </body>
</html>
